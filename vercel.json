{
    "buildCommand": "cd website && npm install && npm run build",
    "outputDirectory": "website/build",
    "framework": null,
    "functions": {
        "api/index.py": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/v1/(.*)",
            "destination": "/api/index.py"
        }
    ]
}